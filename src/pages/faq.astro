---
import faqs from '../data/faqData'
import LayoutPost from '../layouts/LayoutPost.astro'
import Title from '../components/Title.astro'
import FaqItem from "../components/FaqItem.astro";
---
<LayoutPost title="FAQ">
    <section>
        <Title title="Preguntas frecuentes"/>
        {
            faqs.map(itemfaq => (
                <FaqItem item={itemfaq} />
            ))
        }
    </section>
</LayoutPost>

<style>
    section{
        color:white;
        margin-left: auto;
        margin-right: auto;
    }
</style>